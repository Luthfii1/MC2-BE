{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "rootDir": "./",
    "module": "commonjs",
    "target": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./api/*.ts"]
}
